{
	"name": "sewmyheadon/spurs",
	"description": "Combines Understrap, Underscores, and Bootstrap 4, with helpful build tools",
	"type": "wordpress-theme",
	"license": "GPL-2.0",
	"minimum-stability": "stable",
	"keywords": [
		"wordpress",
		"theme",
		"bootstrap",
		"underscores",
		"understrap"
	],
	"homepage": "https://github.com/sewmyheadon/spurs",
	"require": {
		"composer/installers": "^1.7"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"wp-coding-standards/wpcs": "^2.2.0",
		"phpcompatibility/php-compatibility": "^9.3.4"
	},
	"scripts": {
		"test": [
			"vendor/bin/phpcs"
		],
		"fix": [
			"vendor/bin/phpcbf"
		]
	}
}
